{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": [
            true,
            "never-prefix"
        ],
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-leading-underscore"
        ],
        "no-console": false,
        "no-unused-expression": false,
        "semicolon": false
    },
    "rulesDirectory": []
}